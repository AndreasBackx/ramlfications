#%RAML 0.8
title: API Demo - Primative Param Types
version: v1
protocols: [ HTTPS ]
baseUri: https://api.example.com/{version}
mediaType: application/json

/barfoo:
  displayName: barfoo
  get:
    description: Totally uncreative get-barfoo description
    queryParameters:
      invalidParamType:
        displayName: FooBarType
        description: Some Invalid Primative Parameter Type
        type: foobar
        required: false
        repeat: false
      validParamType:
        displayName: StringType
        description: Some Valid Primative Parameter Type
        type: string
        required: false
        repeat: false



