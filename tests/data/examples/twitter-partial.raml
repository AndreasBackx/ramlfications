  # Tweets
  /retweets/{id}{mediaTypeExtension}:
    uriParameters:
      id:
        description: The numerical ID of the desired status.
        type: integer
    type: base
    get:
      is: [ trimmable ]
      description: |
        Returns a collection of the 100 most recent retweets of the tweet specified
        by the id parameter.
      queryParameters:
